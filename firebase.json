{
  "hosting": {
    "public": "dist",
    "site": "calm-the-calamities",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/public/benchmark1/index.html"
      },
      {
        "source": "/benchmark1",
        "destination": "/public/benchmark1/index.html"
      },
      {
        "source": "/benchmark3",
        "destination": "/public/benchmark3/index.html"
      }
    ],
    "redirects":[
      {
      "source": "/benchmark2",
      "destination": "https://calm-the-calamities.firebaseapp.com/",
      "type": 301
      }
    ]
  }
}
